{
  "schemaVersion": 1,
  "id": "subathon",
  "version": "${version}",
  "name": "Sub-a-thon",
  "description": "Minecraft but your Twitch chat can torture you with subs",
  "authors": [
    "Awakened Redstone"
  ],
  "contact": {
    "repo": "https://githu.com/Awakened-Redstone/Subathon"
  },
